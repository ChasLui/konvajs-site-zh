<article class="page" itemscope itemtype="http://schema.org/Article">
  <header id="page-banner">
    <div class="outer">
      <div id="page-banner-inner">
        <h1 id="page-title" itemprop="name"><%= page.title %></h1>
        <a
          href="https://github.com/konvajs/site/blob/master/source/<%- page.path.replace(/\.html$/, '.md') %>"
          title="Improve this doc"
          id="page-edit-link"
        ></a>
      </div>
      <%- partial('_partial/subscribe') %>
    </div>
  </header>

  <div class="outer">
    <div id="mobile-menu-wrap"></div>
    <%- partial('_partial/react-sidebar') %>
    <div class="page-content">
      <!--<div class="toc-wrap">-->
      <!--<strong class="toc-title">Contents</strong>-->
      <!--<%- toc(page.content) %>-->
      <!--</div>-->
      <div class="page-entry" itemprop="articleBody">
        <%- page.content %>
      </div>
      <footer id="page-footer">
        <%- page_nav() %>
      </footer>
      <%- partial('_partial/comment') %>
    </div>
  </div>
</article>
